if=\"show\">Show ........</h3>\r\n    <h3 v-else>Show .......Else.</h3>\r\n\r\n    <button v-on:click=\"hello(data)\">Greeting</button>\r\n    <div v-if=\"show\">\r\n      <button v-on:click=\"display()\">Hide</button>\r\n    </div>\r\n    <div v-else>\r\n      <button v-on:click=\"display()\">Show</button>\r\n    </div>\r\n    <!-- for loop -->\r\n    <table border=\"1px\">\r\n        <tr>\r\n            <th>ID</th>\r\n            <th>Name</th>\r\n            <th>Email</th>\r\n        </tr>\r\n        <tr v-for=\"user in users\" :key=\"user.id\">\r\n            <td>{{user.id}}</td>\r\n            <td>{{user.name}}</td>\r\n            <td>{{user.email}}</td>\r\n        </tr>\r\n    </table>\r\n    <!-- Templete -->\r\n    <p v-html=\"header\"></p>\r\n    \r\n    <button v-on:click=\"clickMe\" v-bind:disabled=\"!btnShow\">Click ME</button>\r\n\r\n    <div>\r\n      <h2 class=\"vueStyle\">Class binding</h2>\r\n      <h2 v-bind:style=\"{color:colorName}\">style binding</h2>\r\n      <h2 v-bind:class=\"{display:show,vueStyle:show}\">Class with Object</h2>\r\n      <h2 v-bind:class=\"[checkDisplay?d : v]\">Class with Array</h2>\r\n    </div>   \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Home\",\r\n  props: {\r\n    data: String,\r\n    msg: String,\r\n    \r\n  },\r\n  methods: {\r\n    hello(name) {\r\n      console.log(\"Hii \" + name);\r\n    },\r\n    display() {\r\n      this.show = !this.show;\r\n    },\r\n    clickMe(){\r\n       this.name = \"DA\"\r\n       this.btnShow = !this.btnShow\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      name:\"\",\r\n      header:\"<h3>Templete</h3>\",\r\n      btnShow:true,\r\n      show: true,\r\n      d:'display',\r\n      v:'vueStyle',\r\n      colorName:'skyblue',\r\n      checkDisplay:true,\r\n      users: [\r\n        {\r\n          id: 1,\r\n          name: \"Đỗ Văn Đạt\",\r\n          email: \"dat98hp@gmail\",\r\n        },\r\n        {\r\n          id: 2,\r\n          name: \"Đặng Thanh Sơn 1\",\r\n          email: \"Son98hp@gmail\",\r\n        },\r\n        {\r\n          id: 3,\r\n          name: \"Đặng Thanh Sơn 2\",\r\n          email: \"Son98hp@gmail\",\r\n        },\r\n        {\r\n          id: 4,\r\n          name: \"Đặng Thanh Sơn 3\",\r\n          email: \"Son98hp@gmail\",\r\n        },\r\n      ],\r\n\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.vueStyle {\r\n  color:brown\r\n}\r\n.display{\r\n  background-color:rgb(235, 96, 124) ;\r\n}\r\n</style>"]}]}