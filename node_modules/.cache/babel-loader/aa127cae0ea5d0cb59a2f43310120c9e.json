   msg: String,\r\n    \r\n  },\r\n  methods: {\r\n    hello(name) {\r\n      console.log(\"Hii \" + name);\r\n    },\r\n    display() {\r\n      this.show = !this.show;\r\n    },\r\n    clickMe(){\r\n       this.name = \"DA\"\r\n       this.btnShow = !this.btnShow\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      name:\"\",\r\n      header:\"<h3>Templete</h3>\",\r\n      btnShow:true,\r\n      show: true,\r\n      d:'display',\r\n      v:'vueStyle',\r\n      colorName:'skyblue',\r\n      checkDisplay:true,\r\n      users: [\r\n        {\r\n          id: 1,\r\n          name: \"Đỗ Văn Đạt\",\r\n          email: \"dat98hp@gmail\",\r\n        },\r\n        {\r\n          id: 2,\r\n          name: \"Đặng Thanh Sơn 1\",\r\n          email: \"Son98hp@gmail\",\r\n        },\r\n        {\r\n          id: 3,\r\n          name: \"Đặng Thanh Sơn 2\",\r\n          email: \"Son98hp@gmail\",\r\n        },\r\n        {\r\n          id: 4,\r\n          name: \"Đặng Thanh Sơn 3\",\r\n          email: \"Son98hp@gmail\",\r\n        },\r\n      ],\r\n\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.vueStyle {\r\n  color:brown\r\n}\r\n.display{\r\n  background-color:rgb(235, 96, 124) ;\r\n}\r\n</style>"],"sourceRoot":""}]}